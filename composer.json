{
  "name": "elastification/php-client-bundle",
  "type": "symfony-bundle",
  "description": "A Elastification client bundle for Symfony2",
  "keywords": ["elasticsearch", "elastification", "php-client", "nosql", "symfony"],
  "homepage": "http://www.elastification.de",
  "license": "MIT",
  "authors": [
    {
      "name": "Daniel Wendlandt",
      "email": "dan.wendlandt@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.4.10",
    "symfony/framework-bundle": ">=2.1,<3.0",
    "symfony/yaml": ">=2.1,<3.0",
    "elastification/php-client": "dev-master"
  },
  "require-dev": {
    "munkie/elasticsearch-thrift-php": "~1.4",
    "guzzlehttp/guzzle": "~4.2",
    "jms/serializer": "~0.15"
  },
  "autoload": {
    "psr-0": { "Elastification\\Bundle\\ElastificationPhpClientBundle": "" }
  },
  "target-dir": "Elastification/Bundle/ElastificationPhpClientBundle",
  "minimum-stability": "dev",
  "suggest": {
    "munkie/elasticsearch-thrift-php": "Needed for thrift transport",
    "guzzlehttp/guzzle": "Needed for HttpGuzzle transport",
    "jms/serializer": "Needed for serialization of entities"
  }
}
